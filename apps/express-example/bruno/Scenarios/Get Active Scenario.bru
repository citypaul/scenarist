meta {
  name: Get Active Scenario
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/__scenario__
  body: none
  auth: none
}

headers {
  x-scenarist-test-id: {{testId}}
}

docs {
  Get the currently active scenario for this test ID.

  Returns:
  - 200: Active scenario details (testId, scenarioId, scenarioName)
  - 404: No active scenario set for this test ID
}
